{
  "manifest_version": 2,
  "name": "ChuckChecker",
  "description": "The RabHelper you love, on crack.",
  "author":"Scott Bremmer, Escher WD",
  "version": "1.1",
  "browser_action": {
    "default_icon": "img/rabIcon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*"
  ],
  "background":{
    "scripts": ["js/background.js"]
  }
}